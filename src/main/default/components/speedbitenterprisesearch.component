            <a class='CoveoTab' data-id='Products' data-caption='Shop' data-expression='@objecttype=="Product"' data-layout="card"></a>
            <a class="CoveoTab" data-id="Downloads" data-caption="Manuals" data-expression='@source==("Sharepoint Speedbit","Box Speedbit")' data-layout="list"></a>
