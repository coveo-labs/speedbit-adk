<apex:component >

    <div id="search" class="CoveoSearchInterface div__enterprisesearch" data-design="new" data-results-per-page="9" data-expression='NOT @filename=(.aspx) NOT @filetype=(spsite,spwikipagelibrarylist,"Box User") NOT @documenttype==(List,Folder,community) NOT @source==("Salesforce SpeedBit") NOT @objecttype=(Thread,Topic,Board,Category,Space) NOT @sfid=="kA16A000000khO4SAI" NOT @connectortype==(Dynamics2Crawler)' data-enable-history="true">

