                <div data-title="Document Type" class="CoveoFacet" data-field="@objecttypename" data-tab="Support" data-enable-collapse="false" data-enable-settings="false"></div>
                
                <div class="CoveoFacet" data-title="Author" data-field="@author" data-tab="Videos" data-enable-collapse="false" data-enable-settings="false"></div>

